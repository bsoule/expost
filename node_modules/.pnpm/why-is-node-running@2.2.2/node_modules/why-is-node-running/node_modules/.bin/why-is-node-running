#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/work/ProgrammingProjects/expost/node_modules/.pnpm/why-is-node-running@2.2.2/node_modules/why-is-node-running/node_modules:/Users/work/ProgrammingProjects/expost/node_modules/.pnpm/why-is-node-running@2.2.2/node_modules:/Users/work/ProgrammingProjects/expost/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/work/ProgrammingProjects/expost/node_modules/.pnpm/why-is-node-running@2.2.2/node_modules/why-is-node-running/node_modules:/Users/work/ProgrammingProjects/expost/node_modules/.pnpm/why-is-node-running@2.2.2/node_modules:/Users/work/ProgrammingProjects/expost/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli.js" "$@"
else
  exec node  "$basedir/../../cli.js" "$@"
fi
